{
    "name": "rias/craft-simple-forms",
    "description": "Forms, made simple",
    "type": "craft-plugin",
    "version": "2.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "forms"
    ],
    "support": {
        "docs": "https://github.com/Marbles/craft-simple-forms/blob/master/README.md",
        "issues": "https://github.com/Marbles/craft-simple-forms/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rias",
            "homepage": "https://rias.be"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0",
        "ext-json": "*",
        "box/spout": "^2.7",
        "tightenco/collect": "^5.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "rias\\simpleforms\\": "src/"
        }
    },
    "extra": {
        "name": "Simple Forms",
        "handle": "simple-forms",
        "schemaVersion": "0.1.2",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/Marbles/craft-simple-forms/master/CHANGELOG.md",
        "components": {
            "simpleForms": "rias\\simpleforms\\services\\SimpleForms",
            "antiSpam": "rias\\simpleforms\\services\\AntiSpam",
            "exports": "rias\\simpleforms\\services\\Exports",
            "install": "rias\\simpleforms\\services\\Install",
            "notes": "rias\\simpleforms\\services\\Notes",
            "recaptcha": "rias\\simpleforms\\services\\Recaptcha",
            "submissions": "rias\\simpleforms\\services\\Submissions",
            "forms": "rias\\simpleforms\\services\\Forms",
            "groups": "rias\\simpleforms\\services\\Groups"
        },
        "class": "rias\\simpleforms\\SimpleForms"
    },
    "require-dev": {
    }
}
